html {
    background: #cfd4d8;
    font-family: century-gothic, sans-serif;
}
div.container {
    border: solid;
    margin-left: 5px;
    padding-left: 9px;
}

.nav a:hover {
    background-color: #ddd;
    font-weight: bold;
    border: solid;
    color: black;
}
.box {
    padding-left:25px;
    padding-top:5px;
    padding-bottom:5px;
    border: solid;
    margin-left: 5px;
    font-size: 30px;
    color: #36383a;
    text-align: center;
}
.line {
    margin-left: 5px;
    background-color: #ba0c2f;
    height: 5px;
}
img.osulogo{
    width:224px;
    height:32px;
    margin-left: 6px;
    margin-top: 10px;
}
.nav {
    margin-left: 18px;
    overflow: hidden;
    font-weight: bold;
    margin: 5px;
    /* justify-content: center; */
    display: flex;
}
.nav a {
    font-size: 35px;
    float: left;
    background-color: #a7b1b7;
    text-decoration : none;
    color: #ba0c2f;
    /* text-align: center; */
    font-weight: bold;
    padding: 14px 16px;
    border: solid;
    border-color: black;
    margin: inherit;

}
.nav a:hover {
    background-color: #ddd;
    font-weight: bold;
    border: solid;
    color: black;
}
