<div id="<%= dom_id presentation %>">
  <p>
    <strong>Title:</strong>
    <%= presentation.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= presentation.description %>
  </p>

  <p>
    <strong>Student:</strong>
    <%= presentation.student_id %>
  </p>

  <p>
    <strong>Event:</strong>
    <%= presentation.event_id %>
  </p>

  <p>
    <strong>Surveys related:</strong>
    <p> avg_score: <%= presentation.surveys.map { |object| object.rating }.reduce(:+) / presentation.surveys.length%>
    <% presentation.surveys.each do |survey| %>
      <p> rating: <%= survey.rating%><p>
      <p> comment:<%= link_to survey.comment, survey%><p>
    <% end %>
  </p>
</div>
