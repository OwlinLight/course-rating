<%= form_for @user do |f| %>
  <%= f.label :email %><br>
  <%= f.text_field :email %><br>
  <br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
  <br>
  <%= f.label :name %><br>
  <%= f.name_field :name %><br>
  <br>
  <%= f.label :user_type %><br>
  <%= f.select :user_type, options_for_select([['Student'], ['Teacher']]) %><br>
  <br>
  <%= f.submit %>
<% end %>
