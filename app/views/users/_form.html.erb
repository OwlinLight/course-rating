<%= form_for @user do |f| %>
  <%= f.label :email %><br>
  <%= f.text_field :email %><br>
  <br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
  <br>
  <%= f.label :character %><br>
  <%= f.select :character, options_for_select([['student'], ['teacher']]) %><br>
  <br>
  <%= f.submit %>
<% end %>
