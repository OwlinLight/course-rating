
<h1>Login</h1>


<%= form_with model: @user do |form| %>

  <%= form.label :email %>
  <%= form.email_field :email %>
  <br><br>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <br><br>
  <%= form.submit "Login" %> <%= link_to "Don't have an account?", signup_path%>

<% end %>